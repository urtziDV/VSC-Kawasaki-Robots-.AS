{
    "name": "kawasaki-as",
    "displayName": "Kawasaki AS",
    "description": "Programming language support for the Kawasaki Robotics .AS",
    "version": "1.0",
    "icon": "images/kawa.png",
    "publisher": "urtziDV",
    "repository": {
        "type": "git",
        "url": "https://github.com/urtziDV/VSCode-Kawasaki-Robotics-.AS"
    },
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "as",
            "aliases": ["KAWASAKI", "Kawasaki"],
            "extensions": [".as"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "as",
            "scopeName": "source.as",
            "path": "./syntaxes/as.tmLanguage.json"
        }]
    }
}